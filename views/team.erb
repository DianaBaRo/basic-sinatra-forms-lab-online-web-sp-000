<!DOCTYPE html>
<html>
 <head>
  <meta charset="UTF-8">
  <title>Basketball Team</title>
 </head>
 <body>
   <h1>Team Name: <%= @team_name %></h1>
   <h3>Coach: <%= @coach %></h3>
   <p>Point Guard: <%= @point_guard %></p>
   <p>Shooting Guard: <%= @shooting_guard %></p>
   <p>Power Forward: <%= @power_forward %></p>
   <p>Small Forward: <%= @small_forward %></p>
   <p>Center: <%= @center %></p>

 </body>
</html>


@team_name = params["name"]
@coach = params["coach"]
@point_guard = params["pg"]
@shooting_guard = params["sg"]
@power_forward = params["pf"]
@small_forward = params["sf"]
@center = params["center"]
